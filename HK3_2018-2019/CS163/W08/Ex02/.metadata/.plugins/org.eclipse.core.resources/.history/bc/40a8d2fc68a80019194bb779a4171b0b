#include<iostream>
#include<vector>
using namespace std;
int main(){
	int n,m;
	int **a;
	int u,v;
	int res=0;
	cin>>n>>m>>u>>v;
	a=new int*[n+1];
	for (int i=1;i<=n;++i)
		a[i]=new int[n+1];
	for (int i=1;i<=n;++i)
		for (int j=1;j<=n;++j)
			cin>>a[i][j];

}


