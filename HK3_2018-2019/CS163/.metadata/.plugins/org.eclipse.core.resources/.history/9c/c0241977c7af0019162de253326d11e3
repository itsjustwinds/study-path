#include"trie.h"
using namespace std;

int Node::get_id(char w){
	for (int i=0;i<(int)had.size();++i)
		if (had[i]==w) return i;
	return -1;
}

int data::get_root(){
	return root;
}

void data::add_word(string S,string file){
	int Now=trie[get_root()];
	for (auto w: S){
		int id=Now.get_id(w);
	}
}
