#pragma once
#ifndef FUNCTION_H_
#define FUNCTION_H_
#include<iostream>
using namespace std;
void input(int &n,int *&a){
	cin>>n;
	for (int i=0;i<n;++i)
		cin>>*(a+i);
	for (int i=0;i<n;++i)
		cout<<*(a+i);
	delete[] a;
}

#endif /* FUNCTION_H_ */
