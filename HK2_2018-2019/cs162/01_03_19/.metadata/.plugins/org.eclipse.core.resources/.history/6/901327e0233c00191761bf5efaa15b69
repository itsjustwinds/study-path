#include"function.h"

void input(Node *root){
	root=new Node;
	Node *cur;
	cur=root;
	while(1){
		int x;
		cin>>x;
		Node *tmp;
		tmp=new Node;
		tmp->val=x;
		tmp->Next=nullptr;
		cur->Next=tmp;
		cur=tmp;
		if (x==0) break;
		cin>>x;
	}
}

