#include<iostream>
using namespace std;
int dfs(int now,int n,int sum,int total,int X){
	if(now==n) {
		if (total+sum==X){
			cout<<"YES";
			return 1;
		}
	}
}
int main(){
	int *a;
	int n,X;
	cin>>n;
	a=new int[n];
	for (int i=0;i<n;++i)
		cin>>a[i];
	if (dfs(0,n,sum,total,X)) cout<<"YES";
	else cout<<"NO";
	return 0;
}


